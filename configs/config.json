{
  "format": "$(BRANCH)$(AHEAD)$(BEHIND)$(SEPARATOR)$(UNTRACKED)$(MODIFIED)$(DELETED)$(UNMERGED)$(STAGED)",
  "includeZeroValues" : false,
  "separator": "|",
  "promptPrefix": "(",
  "promptSuffix": ")",
  "branch" :  {
    "prefix": "$(ESC)[34m",
    "suffix": "$(ESC)[0m"
  },
  "ahead": {
    "prefix": "↑",
    "suffix": ""
  },
  "behind": {
    "prefix": "↓",
    "suffix": ""
  },
  "untracked": {
    "prefix": "+",
    "suffix": ""
  },
  "modified": {
    "prefix": "~",
    "suffix": ""
  },
  "deleted": {
    "prefix": "-",
    "suffix": ""
  },
  "unmerged": {
    "prefix": "x",
    "suffix": ""
  },
  "staged": {
    "prefix": "•",
    "suffix": ""
  }
}
